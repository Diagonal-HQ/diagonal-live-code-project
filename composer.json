{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.4",
    "barryvdh/laravel-debugbar": "^3.15",
    "dyrynda/laravel-cascade-soft-deletes": "^4.5",
    "fakerphp/faker": "^1.24",
    "inertiajs/inertia-laravel": "^2.0",
    "internachi/modular": "^2.3",
    "laravel/framework": "^12.4",
    "laravel/octane": "^2.9",
    "laravel/sanctum": "^4.0",
    "laravel/tinker": "^2.10.1",
    "lorisleiva/laravel-actions": "^2.9",
    "propaganistas/laravel-phone": "^6.0",
    "s-ichikawa/laravel-sendgrid-driver": "^4.0",
    "sendgrid/sendgrid": "^8.1",
    "spatie/eloquent-sortable": "^4.4",
    "spatie/laravel-data": "~4.14.0",
    "spatie/laravel-query-builder": "^6.3",
    "spatie/laravel-ray": "^1.40",
    "spatie/laravel-typescript-transformer": "^2.5",
    "staudenmeir/eloquent-json-relations": "^1.14",
    "tightenco/ziggy": "^2.4",
    "ueberdosis/tiptap-php": "^1.4",
    "utopia-php/domains": "^0.5.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^3.5",
    "friendsofphp/php-cs-fixer": "*",
    "larastan/larastan": "^3.0",
    "laravel/breeze": "^2.3",
    "laravel/pail": "^1.2.2",
    "laravel/pint": "^1.13",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.6",
    "pestphp/pest": "^3.5",
    "pestphp/pest-plugin-laravel": "^3.1",
    "phpstan/phpstan": "^2.1",
    "rector/rector": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "Utils\\Rector\\": "utils/rector/src",
      "Utils\\Rector\\Tests\\": "utils/rector/tests"
    }
  },
  "scripts": {
    "test": "./vendor/bin/pest --parallel --compact",
    "lint": [
      "composer dump-autoload --optimize --strict-psr",
      "composer autocomplete",
      "./vendor/bin/phpstan clear-result-cache",
      "./vendor/bin/phpstan analyse"
    ],
    "fix": [
      "./vendor/bin/rector process",
      "./vendor/bin/pint app"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
    ],
    "autocomplete": [
      "@php artisan ide-helper:generate || true",
      "@php artisan ide-helper:meta || true",
      "@php artisan ide-helper:models -M || true"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
